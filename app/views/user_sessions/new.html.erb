<div id="main_text">
  <h2>Login</h2>
  <% labelled_form_for @user_session, :url => user_session_path do |f| %>
    <%= f.error_messages %>
    <div class="row">
      <%= f.text_field :login %>
    </div>
    <div class="row">
      <%= f.password_field :password %>
    </div>
    <div class="row">
      <%= f.check_box :remember_me %>
    </div>
    <div class="row">
      <%= f.submit "Login" %>
    </div>
  <% end %>
</div>
