<h1>Listing agent_affiliations</h1>

<table>
  <tr>
    <th>Agent</th>
    <th>Agency</th>
    <th>Primary</th>
  </tr>

<% @agent_affiliations.each do |agent_affiliation| %>
  <tr>
    <td><%=h agent_affiliation.agent_id %></td>
    <td><%=h agent_affiliation.agency_id %></td>
    <td><%=h agent_affiliation.primary %></td>
    <td><%= link_to 'Show', admin_agent_affiliation_path(agent_affiliation) %></td>
    <td><%= link_to 'Edit',
      edit_admin_agent_affiliation_path(agent_affiliation) %></td>
    <td><%= link_to 'Destroy', admin_agent_affiliation_path(agent_affiliation),
      :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New agent_affiliation', new_admin_agent_affiliation_path %>
