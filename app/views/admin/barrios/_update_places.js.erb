<% unless lists.empty? %>
  <% javascript_tag do %>
    $('barrio_zone_id').innerHTML='<%= escape_javascript(options_for_select(lists[:zones])) %>';
    $('barrio_province_id').innerHTML='<%= escape_javascript(options_for_select(lists[:provinces])) %>';
    $('barrio_market_id').innerHTML='<%= escape_javascript(options_for_select(lists[:markets])) %>';
  <% end %>
<% end %>

