 <% unless @property.listings.empty? %>  
  <h3>Listings associated with this property</h3>
  <ul>
  <% @property.listings.each do |listing| %>
    <li>
      <%= listing.label %>
      <%= link_to 'Edit', edit_property_listing_path(@property, listing) %>
      <%= link_to 'Delete', property_listing_path(@property, listing),
        :confirm => 'Are you sure?',
        :method => :delete %>
    </li>
  <% end %>
  </ul>
<% end %>
