<h1>New market</h1>

<% labelled_form_for(:admin, @market) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => 'market_fields', 
    :locals => {
      :f => f
    } %>
    
  <div class="row">
    <%= f.submit 'Create' %>
  </div>
<% end %>

<%= link_to 'Back', contextual_collection_path %>
