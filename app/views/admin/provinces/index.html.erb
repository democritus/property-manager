<h1>Listing provinces</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Country</th>
    <th>Position</th>
  </tr>

<% @provinces.each do |province| %>
  <tr>
    <td><%=h province.name %></td>
    <td><%=h province.country.name %></td>
    <td><%=h province.position %></td>
    <td><%= link_to 'Show', contextual_path(province) %></td>
    <td><%= link_to 'Edit', edit_contextual_path(province) %></td>
    <td><%= link_to 'Destroy', contextual_path(province),
      :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New province', new_contextual_path %>
<br />
<% unless params['context_type'].blank? %>
  <%= link_to 'Back to country', context_member_path %>
<% end %>
