<% labelled_form_for( [ :admin, @country ] ) do |f| %>
  <%= f.error_messages %>
  <div class="row">
    <%= f.text_field :name %>
  </div>

  <div class="row">
    <%= f.text_field :iso2 %>
  </div>

  <div class="row">
    <%= f.text_field :position %>
  </div>
  
  <div class="row">
    <%= f.submit 'Submit' %>
  </div>
<% end %>
