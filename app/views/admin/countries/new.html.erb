<h1>New country</h1>

<% labelled_form_for(:admin, @country) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => 'country_fields', 
    :locals => {
      :f => f,
      :lists => @lists
    } %>
    
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', admin_countries_path %>
