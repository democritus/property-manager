<h1>Listing listing_types</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @listing_types.each do |listing_type| %>
  <tr>
    <td><%=h listing_type.name %></td>
    <td><%= link_to 'Show', admin_listing_type_path(listing_type) %></td>
    <td><%= link_to 'Edit', edit_admin_listing_type_path(listing_type) %></td>
    <td><%= link_to 'Destroy', admin_listing_type_path(listing_type),
      :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New listing_type', new_admin_listing_type_path %>
