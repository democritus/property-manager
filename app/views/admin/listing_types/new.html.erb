<h1>New listing_type</h1>

<% labelled_form_for(:admin, @listing_type) do |f| %>
  <%= f.error_messages %>
  
  <%= render :partial => 'listing_type_fields', 
    :locals => {
      :f => f
    } %>
  
  <div class="row">
    <%= f.submit 'Create' %>
  </div>
<% end %>

<%= link_to 'Back', admin_listing_types_path %>
