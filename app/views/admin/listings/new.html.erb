<h1>New listing for <%= @property.name %></h1>

<% labelled_form_for([:admin, @property, @listing]) do |f| %>
  <%= f.error_messages %>
  
  <%= render :partial => 'listing_fields',
    :locals => {
      :f => f
    } %>
    
  <div class="row">
    <%= f.submit 'Create' %>
  </div>
<% end %>

<%= link_to 'Back to property',
  admin_agency_property_path(@property.agency, @property) %>
