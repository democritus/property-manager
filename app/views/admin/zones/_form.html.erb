<% set_zone_form_list_data %>

<% labeled_form_for( [ :admin, @zone ] ) do |f| %>
  <%= f.error_messages %>
  
  <div class="row">
    <%= f.text_field :name %>
  </div>

  <div class="row">
    <%= f.select :country_id, @lists[:countries] %>
  </div>

  <div class="row">
    <%= f.text_field :position %>
  </div>
  
  <div class="row">
    <%= f.submit 'Submit' %>
  </div>
<% end %>

