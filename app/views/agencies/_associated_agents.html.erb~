<% unless @agency.agents.empty? %>  
  <h3>Agents associated with this agency</h3>
  <ul>
  <% @agency.agents.each do |agent| %>
    <li>
      <%= agent.member.login %>
      <%= link_to 'Edit', edit_agency_agent_path(@agency, agent) %>
      <%= link_to 'Delete', agency_agent_path(@agency, agent),
        :confirm => 'Are you sure?',
        :method => :delete %>
    </li>
  <% end %>
  </ul>
<% end %>
