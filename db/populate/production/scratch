SELECT real_estate_listing.PropertyID, real_estate_listing.PropertyName, real_estate_listing.BedroomNum, real_estate_listing.BathNum, real_estate_listing.ConstructionSize, real_estate_listing.LandSize, real_estate_listing.Stories, real_estate_listing.ParkingSpaces, real_estate_listing.YearBuilt, real_estate_listing.AskPrice, real_estate_listing.Headliner_en, real_estate_listing.Summary_en, real_estate_listing.ListingText_en, real_estate_listing.MoreInfo_en, real_estate_listing.AdminNotes, real_estate_listing.ListingType, real_estate_listing.CurrencyCode, property_style.ID FROM real_estate_listing LEFT OUTER JOIN property_style ON real_estate_listing.StyleID = property_style.ID WHERE real_estate_listing.Deleted <> 1 AND real_estate_listing.Hidden = <> 1 AND real_estate_listing.Sold <> 1 ORDER BY real_estate_listing.PropertyID;

SELECT real_estate_listing_category.PropertyID, real_estate_category.CategoryName_en FROM real_estate_listing_category LEFT JOIN real_estate_category ON real_estate_listing_category.CategoryID = real_estate_category.CategoryID ORDER BY real_estate_listing_category.PropertyID;

SELECT real_estate_listing_feature.PropertyID, property_feature.FeatureName FROM real_estate_listing_feature LEFT JOIN property_feature ON real_estate_listing_feature.FeatureID = property_feature.ID ORDER BY real_estate_listing_feature.PropertyID;

SELECT real_estate_listing_images.PropertyID, real_estate_listing_images.ImageName, real_estate_listing_images.ImageType, real_estate_listing_images.Caption, real_estate_listing_images.OrderNum FROM real_estate_listing_images ORDER BY real_estate_listing_images.PropertyID WHERE real_estate_listing_images.Deleted <> 1;
